use uo;
use os;

program corpsedecay (params)

var decayrate := 2700;

detach();
var corpse := params[1];
var acorpsename := corpse.name;
syslog("Corpse: " + acorpsename + " running cleanup.");

sleep(decayrate);

foreach item in EnumerateItemsInContainer (corpse, flags :=0)
	MoveObjectToLocation( item, corpse.x, corpse.y, corpse.z, realm := _DEFAULT_REALM, flags := MOVEOBJECT_NORMAL );
endforeach

DestroyItem(corpse);

endprogram 
